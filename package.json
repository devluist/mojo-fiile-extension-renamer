{
	"name": "mojo-file-extension-renamer",
	"displayName": "Mojify",
	"description": "Just type '.mj' as file extension and it will be renamed to emoji '.ðŸ”¥' or '.mojo' file extension",
	"version": "0.9.0",
	"publisher": "devluist",
	"repository": {
		"url": "https://github.com/devluist/mojo-fiile-extension-renamer"
	},
	"engines": {
		"vscode": "^1.87.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"title": "Mojify",
			"properties": {
				"replaceWith.mojo": {
					"type": "boolean",
					"default": false,
					"description": "Replace .mj extension with .mojo instead of .ðŸ”¥"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "vscode-test"
	},
	"devDependencies": {
		"@types/mocha": "^10.0.6",
		"@types/node": "18.x",
		"@types/vscode": "^1.87.0",
		"@typescript-eslint/eslint-plugin": "^7.0.2",
		"@typescript-eslint/parser": "^7.0.2",
		"@vscode/test-cli": "^0.0.6",
		"@vscode/test-electron": "^2.3.9",
		"eslint": "^8.56.0",
		"typescript": "^5.3.3"
	}
}
